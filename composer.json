{
    "name": "hiqdev/hidev-composer",
    "type": "yii2-extension",
    "description": "Composer plugin for HiDev",
    "keywords": [
        "hidev",
        "composer",
        "packagist",
        "plugin"
    ],
    "homepage": "https://github.com/hiqdev/hidev-composer",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/hidev-composer",
        "issues": "https://github.com/hiqdev/hidev-composer/issues",
        "wiki": "https://github.com/hiqdev/hidev-composer/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "silverfire@hiqdev.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "tafid@hiqdev.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@hiqdev.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "hiqdev/composer-extension-plugin": "<2.0"
    },
    "require-dev": {
        "hiqdev/hidev": "dev-master",
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev-readme": "dev-master",
        "hiqdev/hidev-travis": "dev-master",
        "hiqdev/hidev-vendor": "dev-master",
        "hiqdev/hidev-license": "dev-master",
        "hiqdev/hidev-phpunit": "dev-master",
        "hiqdev/hidev-scrutinizer": "dev-master",
        "hiqdev/hidev-php-cs-fixer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "hidev\\composer\\": "src"
        }
    },
    "extra": {
        "extension-plugin": {
            "hidev-config": "src/hidev-config.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.hiqdev.com"
        }
    ]
}
